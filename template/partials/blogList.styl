.blogList
  list-style: none
  padding: 0
  li
    display: flex
    .date
      flex: 1
      text-align: right
      padding-right: 10px
    a
      flex: 2
      text-decoration: none
      display: flex
      align-items: flex-end;
      &:hover
        text-decoration: underline
    .small
      font-size: 0.6em
      padding-left: 10px
